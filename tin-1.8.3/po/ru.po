	"#   5 = Дата по убыванию\n"
	"# * 6 = Дата по возрастанию\n"
	"#   7 = Score по убыванию\n"
	"#   8 = Score по возрастанию\n"
	"#   9 = Количество строк по убыванию\n"
	"#  10 = Количество строк по возрастанию\n"

